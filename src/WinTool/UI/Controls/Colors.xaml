<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="EmptyBackdropColor">#FF202020</Color>
    <Color x:Key="AcrylicLayoutColor">#99000000</Color>

    <SolidColorBrush x:Key="EmptyBackdropBrush" Color="{StaticResource EmptyBackdropColor}" />
    <SolidColorBrush x:Key="AcrylicLayoutBrush" Color="{StaticResource AcrylicLayoutColor}" />
</ResourceDictionary>